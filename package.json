{
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "build": "esbuild src/server.ts --bundle --outfile=build/server.js --platform=node --format=esm --packages=external",
    "start": "node build/server.js",
    "dev": "tsx watch src/server.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx migrate.ts",
    "db:drop": "drizzle-kit drop",
    "db:pull": "drizzle-kit introspect:pg",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check:pg",
    "db:update": "bun run db:generate && bun run db:migrate",
    "db:seed": "tsx data/seed.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@metaplex-foundation/mpl-token-metadata": "^2.0.0",
    "@react-email/components": "0.0.14",
    "@react-email/render": "0.0.12",
    "@solana/spl-token": "^0.4.6",
    "@solana/web3.js": "^1.93.0",
    "argon2": "0.31.2",
    "consola": "3.2.3",
    "cors": "2.8.5",
    "dotenv": "16.4.1",
    "drizzle-orm": "^0.31.2",
    "drizzle-zod": "0.5.1",
    "express": "4.19.2",
    "express-rate-limit": "7.1.5",
    "jsonwebtoken": "9.0.2",
    "nanoid": "^5.0.7",
    "postgres": "3.4.3",
    "react": "18.2.0",
    "request-ip": "3.3.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.18.0",
    "@stylistic/eslint-plugin": "2.1.0",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.5",
    "@types/node": "20.11.16",
    "@types/react": "18.2.53",
    "@types/request-ip": "0.0.41",
    "drizzle-kit": "0.22.7",
    "esbuild": "0.20.0",
    "eslint": "8.57.0",
    "pg": "8.11.3",
    "tsx": "4.7.0",
    "typescript": "5.3.3"
  }
}
